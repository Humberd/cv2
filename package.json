{
  "name": "resume",
  "description": "Print-optimized React version of my Resume",
  "version": "0.1.0",
  "author": "Maciej Sawicki",
  "license": "MIT",
  "scripts": {
    "build:ci": "gatsby build --prefix-paths",
    "serve:ci": "gatsby serve --prefix-paths",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "pdf:ci": "node pdf.js ci",
    "pdf-sequence:ci": "concurrently --kill-others --success=first \"npm run serve:ci\" \"npm run pdf:ci\"",
    "develop": "gatsby develop",
    "format": "prettier --write src/**/*.{ts,tsx,js,jsx}",
    "start": "npm run develop",
    "deploy-manual": "gatsby build --prefix-paths && gh-pages -d public",
    "pdf": "node pdf.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Humberd/resume"
  },
  "private": false,
  "dependencies": {
    "aproba": "2.0.0",
    "gatsby": "4.1.3",
    "gatsby-image": "3.11.0",
    "gatsby-plugin-manifest": "4.1.2",
    "gatsby-plugin-offline": "5.1.2",
    "gatsby-plugin-react-helmet": "5.1.0",
    "gatsby-plugin-sass": "5.1.1",
    "gatsby-plugin-sharp": "4.1.2",
    "gatsby-plugin-typescript": "4.1.1",
    "gatsby-source-filesystem": "4.1.1",
    "gatsby-transformer-sharp": "4.1.0",
    "prop-types": "15.7.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-helmet": "6.1.0",
    "sass": "1.43.4"
  },
  "devDependencies": {
    "@types/puppeteer": "5.4.4",
    "@types/react-helmet": "6.1.4",
    "concurrently": "6.3.0",
    "gatsby-plugin-no-javascript": "2.0.5",
    "gh-pages": "3.2.3",
    "prettier": "2.4.1",
    "puppeteer": "11.0.0"
  }
}
